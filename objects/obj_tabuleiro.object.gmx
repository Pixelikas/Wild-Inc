                    ds_grid_set(var_tabuleiro,i,j,0); ds_grid_set(var_tabuleiro,h1,j,0); ds_grid_set(var_tabuleiro,h2,j,0); ds_grid_set(var_tabuleiro,h3,j,0);  
                    audio_play_sound(snd_Points,2,false);
                 }else if (ds_grid_get(var_tabuleiro,i,j) == ds_grid_get(var_tabuleiro,h1,j) &amp;&amp; ds_grid_get(var_tabuleiro,i,j) == ds_grid_get(var_tabuleiro,h2,j)){
                    //combinaçao de 3
                    if(global.playerturn == 1){global.player1points += 50} else if (global.playerturn == 2){global.player2points += 50}
                    ds_grid_set(var_tabuleiro,i,j,0); ds_grid_set(var_tabuleiro,h1,j,0); ds_grid_set(var_tabuleiro,h2,j,0);
                    audio_play_sound(snd_Points,2,false);
                }
            }
            if(i==2){
                h1=i+1;h2=i+2;h3=i+3;
                if (ds_grid_get(var_tabuleiro,i,j) == ds_grid_get(var_tabuleiro,h1,j) &amp;&amp; ds_grid_get(var_tabuleiro,i,j) == ds_grid_get(var_tabuleiro,h2,j) &amp;&amp; ds_grid_get(var_tabuleiro,i,j) == ds_grid_get(var_tabuleiro,h3,j)){
                    //combinaçao de 4
