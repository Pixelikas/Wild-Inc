                    ds_grid_set(var_tabuleiro,i,j,0); ds_grid_set(var_tabuleiro,h1,j,0); ds_grid_set(var_tabuleiro,h2,j,0); ds_grid_set(var_tabuleiro,h3,j,0); ds_grid_set(var_tabuleiro,h4,j,0); 
                    audio_play_sound(snd_Points,2,false);
                }else if (ds_grid_get(var_tabuleiro,i,j) == ds_grid_get(var_tabuleiro,h1,j) &amp;&amp; ds_grid_get(var_tabuleiro,i,j) == ds_grid_get(var_tabuleiro,h2,j) &amp;&amp; ds_grid_get(var_tabuleiro,i,j) == ds_grid_get(var_tabuleiro,h3,j)){
