                    ds_grid_set(var_tabuleiro,i,j,0); ds_grid_set(var_tabuleiro,h1,j,0); ds_grid_set(var_tabuleiro,h2,j,0); ds_grid_set(var_tabuleiro,h3,j,0);  
                    audio_play_sound(snd_Points,2,false);
                }else if (ds_grid_get(var_tabuleiro,i,j) == ds_grid_get(var_tabuleiro,h1,j) &amp;&amp; ds_grid_get(var_tabuleiro,i,j) == ds_grid_get(var_tabuleiro,h2,j)){
                    //combinaçao de 3
                    if(global.playerturn == 1){global.player1points += 50} else if (global.playerturn == 2){global.player2points += 50}
                    ds_grid_set(var_tabuleiro,i,j,0); ds_grid_set(var_tabuleiro,h1,j,0); ds_grid_set(var_tabuleiro,h2,j,0);
                    audio_play_sound(snd_Points,2,false);
                }
            }
            if(i==3){
                h1=i+1;h2=i+2;
                if (ds_grid_get(var_tabuleiro,i,j) == ds_grid_get(var_tabuleiro,h1,j) &amp;&amp; ds_grid_get(var_tabuleiro,i,j) == ds_grid_get(var_tabuleiro,h2,j)){
                    //combinaçao de 3
                    if(global.playerturn == 1){global.player1points += 50} else if (global.playerturn == 2){global.player2points += 50}
                    ds_grid_set(var_tabuleiro,i,j,0); ds_grid_set(var_tabuleiro,h1,j,0); ds_grid_set(var_tabuleiro,h2,j,0);
                    audio_play_sound(snd_Points,2,false);
                }
            }
        
        }
    }
    
if(ds_grid_get(var_tabuleiro,0,0) !=0 &amp;&amp; ds_grid_get(var_tabuleiro,1,0) !=0 &amp;&amp; ds_grid_get(var_tabuleiro,2,0) !=0 &amp;&amp; ds_grid_get(var_tabuleiro,3,0) !=0 &amp;&amp; ds_grid_get(var_tabuleiro,4,0) !=0 &amp;&amp; ds_grid_get(var_tabuleiro,5,0) !=0){
    //game over fucker!
    for(i=0;i&lt;6;i++){
        for(j=0;j&lt;9;j++){
            ds_grid_set(var_tabuleiro,i,j,item.none);
        }
    }
    audio_stop_sound(snd_Ambience)//desligar musica de jogo
    if(global.player1points &gt; global.player2points){//vitoria jogador 1
        obj_player1.pl_animation = anim.win;
        obj_player2.pl_animation = anim.lose;
        if(global.player1char == char.bunny){
            audio_play_sound(snd_Hefner,3,false)
            global.gameover = true;//finaliza o jogo
        }
        if(global.player1char == char.squirel){
            audio_play_sound(snd_Noah,3,false)
            global.gameover = true;//finaliza o jogo
        }
        if(global.player1char == char.bear){
            audio_play_sound(snd_Olga,3,false)
            global.gameover = true;//finaliza o jogo
        }
        if(global.player1char == char.quati){
            audio_play_sound(snd_Kaua,3,false)
            global.gameover = true;//finaliza o jogo
        }
