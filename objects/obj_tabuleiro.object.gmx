            if(i==0 || i==1){
                h1=i+1;h2=i+2;h3=i+3;h4=i+4;
                if(ds_grid_get(var_tabuleiro,i,j) == ds_grid_get(var_tabuleiro,h1,j) &amp;&amp; ds_grid_get(var_tabuleiro,i,j) == ds_grid_get(var_tabuleiro,h2,j) &amp;&amp; ds_grid_get(var_tabuleiro,i,j) == ds_grid_get(var_tabuleiro,h3,j) &amp;&amp; ds_grid_get(var_tabuleiro,i,j) == ds_grid_get(var_tabuleiro,h4,j)){
                    //combinaçao de 5
